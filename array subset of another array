string isSubset(int a1[], int a2[], int n, int m) {
    sort(a1,a1+n);
   sort(a2,a2+m);
   int count=0;
   int i=0,j=0;
   while(i<n and j<m)
   {
       if(a1[i]==a2[j])
       {
           count++;
           i++;
           j++;
       }
       else if(a1[i]<a2[j])
       {
           i++;
       }
       else{
           j++;
       }
   }
   if(count==m)    return "Yes";
   else    return "No";
    }
